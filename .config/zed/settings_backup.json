{
  "vim_mode": true,
  "relative_line_numbers": true,
  "autosave": "on_focus_change",
  "cursor_shape": "block",
  "vim": {
    "use_clipboard": "always",
    "use_smartcase_search": true,
  },
  "telemetry": {
    "diagnostics": true,
    "metrics": true,
  },
  "icon_theme": "Zed (Default)",
  "ui_font_size": 16,
  "buffer_font_size": 15,
  "theme": {
    "mode": "dark",
    "light": "One Light",
    "dark": "One Dark",
  },
}
